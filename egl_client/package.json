{
    "name": "with-tailwindcss",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "backup-abis": "FOLDER=abi-history/$(date +%Y%m%d_%H%M%S); mkdir -p $FOLDER && cp lib/contracts/* $FOLDER",
        "prep-abis": "mkdir -p lib/contracts; cp ../egl_genesis/build/contracts/Egl* lib/contracts && cp ../egl/build/contracts/Egl* lib/contracts",
        "link-abis": "yarn backup-abis && yarn prep-abis",
        "dev": "TAILWIND_MODE=watch NODE_ENV=development next dev",
        "dev-fresh": "yarn link-abis && TAILWIND_MODE=watch NODE_ENV=development next dev",
        "build": "next build && next export",
        "build-fresh": "yarn link-abis && next build && next export",
        "build-client": "next build && next export",
        "start": "next start",
        "start-fresh": "yarn link-abis && next start",
        "rmabi": "rm -rf lib/contracts/*"
    },
    "dependencies": {
        "@types/web3": "^1.2.2",
        "bignumber.js": "^9.0.1",
        "classnames": "^2.3.1",
        "clsx": "^1.1.1",
        "copy-to-clipboard": "^3.3.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "next": "latest",
        "react": "^17.0.1",
        "react-add-to-calendar": "^0.1.5",
        "react-add-to-calendar-hoc": "^1.0.10",
        "react-dom": "^17.0.1",
        "react-draggable": "^4.4.3",
        "react-hook-form": "^7.6.2",
        "react-jazzicon": "^0.1.3",
        "react-switch": "^6.0.0",
        "styled-components": "^5.3.0",
        "web3": "^1.3.6",
        "axios": "^0.21.1"
    },
    "devDependencies": {
        "@types/react": "^17.0.5",
        "autoprefixer": "^10.0.4",
        "postcss": "^8.1.10",
        "tailwindcss": "^2.1.1",
        "typescript": "^4.2.4"
    }
}
